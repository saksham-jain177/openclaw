{
  "name": "openclaw",
  "version": "2026.2.1",
  "description": "Personal Ops Automation Agent skeleton (forked from OpenClaw)",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "bin": {
    "openclaw": "openclaw.mjs"
  },
  "files": [
    "LICENSE",
    "openclaw.mjs",
    "README.md",
    "dist/*.js",
    "dist/*.json"
